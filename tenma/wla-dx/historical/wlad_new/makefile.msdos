
CC = gcc
CFLAGS = -m486 -c -ansi -O3 -pedantic -Wall -DMSDOS -DGB
LD = gcc
LDFLAGS = 

CFILES = main.c
HFILES = main.h
OFILES = main.o

all: $(OFILES) makefile
	$(LD) $(LDFLAGS) $(OFILES) -o wlad ; strip wlad

main.o: main.c main.h makefile
	$(CC) $(CFLAGS) main.c


$(OFILES): $(HFILES)


clean:
	del *.o

install:
	make ; cp wlad /usr/local/bin
